<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mosericko.aflewo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".member.UserProfile"></activity>
        <activity android:name=".marketing.Marketing" />
        <activity android:name=".fundraiser.Fundraiser" />
        <activity android:name=".financemanager.FinanceManager" />
        <activity android:name=".eventsmanager.EventsManager"
            android:theme="@style/UserRegistration"/>
        <activity android:name=".loginregistration.MoreDetails" />
        <activity android:name=".loginregistration.UserLogin" />
        <activity
            android:name=".loginregistration.UserRegistration"
            android:theme="@style/AppTheme" />
        <activity android:name=".loginregistration.SignActivityPrompt">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".member.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>